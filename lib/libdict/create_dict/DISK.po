#
# code: DISK-SCSI-01
# keys: ereport.disk.unhealthy
#
msgid "ereport.disk.unhealthy"
msgstr "DISK-SCSI-01"
msgid "DISK-SCSI-01.class"
msgstr "ereport.disk.unhealthy"
msgid "DISK-SCSI-01.type"
msgstr "Error"
msgid "DISK-SCSI-01.severity"
msgstr "Major"
msgid "DISK-SCSI-01.description"
msgstr "A level 0 cache on this cpu is faulty. Refer to http://sun.com/msg/GMCA-8000-47 for more information."
msgid "DISK-SCSI-01.response"
msgstr "The system will attempt to offline this cpu to remove it from service."
msgid "DISK-SCSI-01.impact"
msgstr "Performance of this system may be affected."
msgid "DISK-SCSI-01.action"
msgstr "Schedule a repair procedure to replace the affected CPU. Use 'fmadm faulty' to identify the module."
